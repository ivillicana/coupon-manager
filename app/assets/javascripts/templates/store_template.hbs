<div class="text-center">
  <h1 class="h1 mb-3">{{name}}</h1>
  <button class="btn btn-sm btn-primary btn-block mb-5 form-signin login-2" id="new-store-coupon" data-storeid="{{id}}">New {{name}} Coupon</button>
</div>


<div class="text-center container-list">
  {{#each coupons}}
    <ul class="list-unstyled mb-5">
      <h1 class="h4 text-left"><a href="/coupons/{{id}}" class="coupon-link" data-couponId="{{id}}">{{item}}</a></h1>
      <ul class="list-unstyled text-left">
        <li>{{ offer_description }}</li>
      </ul>
    </ul>
  {{/each}}
</div>