<h1>Log In</h1>

<%= form_for :user, url: login_path, method: :post do |f| %>
  <%= f.label :username %>
  <%= f.text_field :username %><br>

  <%= f.label :password %>
  <%= f.password_field :password %><br></br>

  <%= f.submit "Log In" %><br></br>
  Or<br></br> 
  <%= link_to "Sign Up", new_user_path %>
<% end %>
<div>
  <%= link_to('Log in with Facebook!', '/auth/facebook') %>
</div>