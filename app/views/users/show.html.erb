
<div class="container text-center">
  <h3><%= @user.name %></h3>
  <p><%= @user.email %></p>
  <div class="media">
    <img class="img-thumbnail mx-auto d-block" src="<%= @user.try(:image) %>">
  </div>
</div>

<div class="container">
  <% current_coupon_folder.each do |coupon_id| %>
    <% coupon = Coupon.find_by(id: coupon_id) %>
    <ul>
      <p><%= link_to coupon.item, coupon_path(coupon) %></p>
      <li><%= coupon.store_name %></li>
    </ul>
  <% end %>
</div>

